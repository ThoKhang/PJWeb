            clickable: true,
            dynamicBullets: true,
        },

        // Navigation
        navigation: {
            nextEl: ".swiper-button-next",
            prevEl: ".swiper-button-prev",
        },

        // Lazy loading
        lazy: true,

        // Keyboard control
