    <style>
        /* RESET */
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            background: #ffffff; /* Nền trang TRẮNG */
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }

        /* Slider Container ==================================================================*/
        .slider-wrapper {
            width: 100%;
            margin: 0;
            padding: 0;
            overflow: hidden;
            background: #ffffff; /* nền trắng */
        }

        .mySwiper {
            width: 100%;
            height: auto;
            position: relative;
            aspect-ratio: 16 / 6.7; /* tỷ lệ banner */
            background: #ffffff !important; /* ép TRẮNG */
        }

        /* Responsive */
        @@media (max-width: 1200px) {
            .mySwiper {
                aspect-ratio: 16 / 6;
            }
        }

        @@media (max-width: 768px) {
            .mySwiper {
                aspect-ratio: 16 / 7;
            }
        }

        @@media (max-width: 480px) {
            .mySwiper {
                aspect-ratio: 16 / 9;
            }
        }

        /* Slides */
        .swiper-slide {
            display: flex;
            justify-content: center;
            align-items: center;
            background: #ffffff !important; /* Xóa màu xám mặc định */
        }

            .swiper-slide img {
                width: 100%;
                height: 100%;
                object-fit: contain; /* giữ nguyên tỷ lệ hình */
                object-position: center;
                transition: transform 0.6s ease;
                border-radius:20px;
            }

        /* Zoom effect */
        .swiper-slide-active img {
            transform: scale(1.04);
        }

        /* Overlay (giảm opacity để nền trắng không bị tối) */
        .slide-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: linear-gradient(to bottom, rgba(255,255,255,0.0), rgba(255,255,255,0.0));
            z-index: 1;
        }

        /* Navigation Buttons */
        .swiper-button-next,
        .swiper-button-prev {
            width: 46px;
            height: 46px;
            background: rgba(255, 255, 255, 0.95);
            border-radius: 50%;
            color: #6A1B9A !important;
            box-shadow: 0 4px 15px rgba(106, 27, 154, 0.3);
            transition: 0.25s ease;
        }

            .swiper-button-next:hover,
            .swiper-button-prev:hover {
                background: #6A1B9A;
                color: #ffffff !important;
                transform: scale(1.12);
            }

            .swiper-button-next::after,
            .swiper-button-prev::after {
                font-size: 20px;
                font-weight: bold;
            }

        /* Smaller on mobile */
        @@media (max-width: 768px) {
            .swiper-button-next,
            .swiper-button-prev {
                width: 40px;
                height: 40px;
            }

                .swiper-button-next::after,
                .swiper-button-prev::after {
                    font-size: 14px;
                }
        }

        /* Pagination */
        .swiper-pagination {
            bottom: 15px !important;
        }

        .swiper-pagination-bullet {
            width: 12px;
            height: 12px;
            background: linear-gradient(to right, #6A1B9A, #5BB0DA);
            opacity: 1;
            transition: 0.3s;
        }

        .swiper-pagination-bullet-active {
            width: 36px;
            height: 12px;
            border-radius: 6px;
            background: #6A1B9A;
        }
    </style>
